<%= @project.name %>
<%= @project.status %>
<% if @project.status == "complete" %>
 Rate This Professional: <%= @professional.name %>
 <%= form_for [@requester,@rating],  url: requester_ratings_path(professional: @professional) do |f| %>
   <%= f.text_field :score  %>
   <%= f.label "Score" %>
   <%= f.text_area :comment %>
   <%= f.label "Comment" %>
   <%= f.submit %>
<% end %>
<% @project.proposals.each do |proposal| %>
  <p>Proposal ID: <%= link_to proposal.id, requester_proposal_path(proposal) %></p>
  <p>From: <%= proposal.user.name %></p>
<% end %>
